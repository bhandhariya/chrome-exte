{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "browser_action":{
        "default_popup":"index.html"
    },
    "permissions": [
        "activeTab",
        "webNavigation",
        "storage",
        "tabs"
    ],
    "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
    "web_accessible_resources":["*.jpg"]
}
  